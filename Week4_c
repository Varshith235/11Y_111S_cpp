#include <iostream>
using namespace std;
class Box {
private:
    int length;
public:
    Box(int l) {
        length = l;
    }
    friend void showLength(Box);
};
void showLength(Box b) {
    cout << "Length of the box: " << b.length << endl;
}
int main() {
    Box smallBox(25);
    showLength(smallBox);
    return 0;
}
